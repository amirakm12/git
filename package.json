{
  "name": "ai-image-restoration-app",
  "version": "1.0.0",
  "description": "AI-powered image restoration and enhancement app with multiple AI agents",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup": "node start.js",
    "test": "node test-setup.js",
    "demo": "node demo.js",
    "cleanup": "node -e \"require('fs').rmSync('uploads', {recursive: true, force: true}); require('fs').rmSync('outputs', {recursive: true, force: true}); console.log('Cleanup complete')\"",
    "install-deps": "npm install",
    "build": "echo 'No build step required for Node.js app'",
    "postinstall": "node -e \"if (!require('fs').existsSync('.env')) { require('fs').copyFileSync('.env.example', '.env'); console.log('Created .env from template') }\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "sharp": "^0.32.6",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "openai": "^4.20.1",
    "replicate": "^0.22.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "image",
    "restoration",
    "ai",
    "enhancement",
    "gfpgan",
    "real-esrgan",
    "openai",
    "replicate",
    "landmark",
    "historic",
    "photo",
    "enhancement"
  ],
  "author": "AI Image Restoration Team",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/ai-image-restoration-app.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/ai-image-restoration-app/issues"
  },
  "homepage": "https://github.com/your-username/ai-image-restoration-app#readme"
}